<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.shaw.demo.mapper.UserMapper">

    <select id="getUserByState" resultType="com.shaw.demo.entity.User">
        select * from xtgl_user
        <where>
              and state = #{state}
        </where>
    </select>



</mapper>
